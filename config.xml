<?xml version='1.0' encoding='utf-8'?>
<widget id="com.alberione.lukas" version="1.1.0" xmlns:gap="http://phonegap.com/ns/1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lukas</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
	
	<plugin name="cordova-plugin-camera" spec="1.2.0" source="pgb" />
	<plugin name="cordova-plugin-file" spec="3.0.0" source="pgb" />
	<plugin name="org.apache.cordova.file-transfer" spec="0.5.0" source="pgb" />
	<plugin name="org.apache.cordova.network-information" spec="0.2.15" source="pgb" />
	<plugin name="com.phonegap.plugins.facebookconnect" spec="0.9.0" source="pgb">
		<param name="APP_ID" value="1545172195712716" />
		<param name="APP_NAME" value="Lukas" />
	</gap:plugin>
</widget>
